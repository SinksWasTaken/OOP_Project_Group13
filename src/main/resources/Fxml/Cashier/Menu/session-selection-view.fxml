<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.group13.Controllers.Cashier.SelectOperations.CashierDateSelectionController"
            styleClass="inventory-anchor" stylesheets="@../../../Styles/manager/inventory-view.css">
    <VBox alignment="TOP_CENTER" layoutX="50.0" layoutY="25.0" prefHeight="750.0" prefWidth="800.0" spacing="10.0" styleClass="form-container">
        <Label styleClass="form-title" text="View / Edit Inventory" />
        <TableView fx:id="sessionsTable" prefHeight="600.0" prefWidth="800.0" styleClass="inventory-table">
            <columns>
                <TableColumn fx:id="dateColumn" text="Date"/>
                <TableColumn fx:id="timeColumn" text="Time"/>
                <TableColumn fx:id="hallColumn" text="Hall Number"/>
            </columns>
        </TableView>
        <HBox prefHeight="100.0" prefWidth="200.0" styleClass="hbox-centered">
            <Button fx:id="cancelButton" layoutX="14.0" layoutY="230.0" mnemonicParsing="false" styleClass="form-button" text="Cancel"/>
            <Button fx:id="nextStageButton" layoutX="100.0" layoutY="230.0" mnemonicParsing="false" styleClass="delete-button" text="Next"/>
        </HBox>
    </VBox>
</AnchorPane>